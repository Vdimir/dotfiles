{
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "editor.tokenColorCustomizations": {
        "[Base16 Light Grayscale]": {
            "functions": "#404060",
            "variables": "#404060",
            "keywords": "#303030",
            "types": "#606060",
            "strings": "#606060"
        }
    },
    "workbench.colorCustomizations": {
        "[Base16 Light Grayscale]": {
            "editor.background": "#FCFEFE",
            "editorCursor.foreground": "#464646",
            "editor.foreground": "#303030",
            "editor.wordHighlightBorder": "#303030",
            // "editor.selectionHighlightBackground": "#00000000",
            "editor.selectionHighlightBorder": "#ababab55",
            "editor.findMatchHighlightBackground": "#FFEEAAAA",
            "editor.findMatchBackground": "#FFDDAA",
        }
    },
    "editor.rulers": [140],
    "editor.fontFamily": "'Fira Code'",
    "editor.fontSize": 13,
    "editor.fontLigatures": true,
    "files.hotExit": "off",
    "files.autoSave": "onFocusChange",
    "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?",
    "editor.occurrencesHighlight": false,
    "search.location": "panel",
    "window.zoomLevel": 1,
    "editor.renderWhitespace": "selection",
    "workbench.sideBar.location": "left",
    "git.ignoreLegacyWarning": true,
    "files.trimTrailingWhitespace": true,
    "window.restoreWindows": "none",
    "editor.codeLens": false,
    "git.autorefresh": false,
    "editor.minimap.enabled": true,
    "workbench.colorTheme": "Base16 Light Grayscale",
}