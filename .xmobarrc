Config { font = "xft:DejaVu Sans Mono for Powerline:size=10"
        , borderColor = "black"
        , border = TopB
        , bgColor = "#1D1F21"
        , fgColor = "grey"
        , position = TopW L 100
        , commands = [
                Run Battery [ "-t", "AC: <acstatus> <left>%"
                                    , "-h", "#859900"
                                    , "-n", "#859900"
                                    , "-l", "#dc322f"
                                    , "--"
                                    , "-O", "charging"
                                    , "-i", "connected"
                                    , "-o", "off"
                           ] 60
                        , Run Date "%H:%M" "date" 10
                        , Run StdinReader
                        ]
        , sepChar = "%"
        , alignSep = "}{"
        , template = "  %StdinReader% }{%battery% | %date%  "
        }
