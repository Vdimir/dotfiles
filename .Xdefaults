
Xft.dpi: 96
Xft.hinting: 1
Xft.hintstyle: hintfull
!Xft.autohint: 0
!! lcdnone, lcddefault, lcdlight, lcdlegacy
Xft.lcdfilter: lcddefault
Xft.rgba: rgb
Xft.antiatlas: 1

URxvt.depth: 32
URxvt.scrollBar: false

URxvt.font: xft:DejaVu Sans Mono for Powerline:size=10.5
URxvt.letterSpace: -2
